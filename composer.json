{
  "name": "arraypress/wp-math-utils",
  "version": "1.0.0",
  "description": "A lean WordPress library for mathematical calculations, e-commerce operations, and number utilities",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-math-utils/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\MathUtils\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-math-utils/issues",
    "source": "https://github.com/arraypress/wp-math-utils"
  },
  "keywords": [
    "wordpress",
    "math",
    "mathematics",
    "ecommerce",
    "calculations",
    "numbers",
    "discount",
    "tax",
    "commission",
    "utilities",
    "arraypress"
  ]
}